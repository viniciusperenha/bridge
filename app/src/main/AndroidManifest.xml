<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.impactit.bridgeengenharia" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:noHistory="true"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProducaoActivity"
            android:label="@string/title_activity_producao"
            android:noHistory="true">
        </activity>
        <activity
            android:name=".ArquivosActivity"
            android:label="@string/title_activity_arquivos"
            android:noHistory="true">
        </activity>
        <activity
            android:name=".PrincipalActivity"
            android:label="@string/title_activity_principal"
            android:noHistory="true">
        </activity>
        <activity
            android:name=".QualidadeActivity"
            android:label="@string/title_activity_qualidade"
            android:noHistory="true" >
        </activity>
    </application>


</manifest>
